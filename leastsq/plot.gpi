set term svg
set out "plot.svg"
set termoption dash
set style line 1 lt 1 lw 1 pt 1 lc 1
set style line 2 lt 1 lw 1 pt 3 lc 2
set style line 3 lt 3 lw 1 pt 3 lc 3

plot 'outA.txt' i 0 w errorbars ls 1 title 'data points'

plot 'outA.txt' i 1 u 1:2 w l ls 2 title 'fit',\
     'outA.txt' i 1 u 1:3 w l ls 3 title 'Upper bound',\
     'outA.txt' i 1 u 1:4 w l ls 3 title 'Lower bound'
